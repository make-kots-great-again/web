{
    "userId": {
        "type": "UUIDV4",
        "defaultValue": "UUIDV4",
        "allowNull": false,
        "primaryKey": true
    },
    "username": {
        "type": "string",
        "unique": true,
        "allowNull": false,
        "validate": {"len": [4, 32]}
    },
    "firstName": {
        "type": "string",
        "allowNull": false,
        "validate": {"len": [4, 32]}
    },
    "lastName": {
        "type": "string",
        "allowNull": false,
        "validate": {"len": [4, 32]}
    },
    "email": {
        "type": "string",
        "unique": true,
        "allowNull": false,
        "isEmail": true
    },
    "password": {
        "type": "string",
        "allowNull": false,
        "notEmpty": true
    }
}
