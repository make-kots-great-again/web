{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "node": "12.14.1"
  },
  "description": "back end server with node.js",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "npm run install",
    "start": "babel-node app.js",
    "angular:build": "cd client-side && npm run build",
    "db:setup": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "start:dev": "npm run db:setup && nodemon -r dotenv/config --exec \"npm start\"",
    "dev": "nodemon -r dotenv/config --exec \"npm start\"",
    "test:integration": "mocha --require @babel/register 'server-side/tests/integration-tests/*.js' --timeout 10000 --exit",
    "test:integration:watch": "nodemon --exec \"npm run test:integration\"",
    "cover:integration:nyc": "nyc --reporter=html --reporter=text-summary --report-dir=test-coverage/integration-tests npm run test:integration",
    "cover:integration:mocha": "mocha --require @babel/register 'server-side/tests/integration-tests/*.js' --timeout 10000 --exit --reporter mochawesome --reporter-options reportDir=test-reports/integration --reporter-options reportFilename=index",
    "test:unit": "NODE_ENV=test mocha --require @babel/register 'server-side/tests/unit-tests/**/*.js' --timeout 10000 --exit",
    "test:unit:watch": "NODE_ENV=test nodemon --exec \"npm run test:unit\"",
    "test:file": "NODE_ENV=test mocha --require @babel/register --timeout 10000 --exit",
    "cover:unit:nyc": "nyc --reporter=html --reporter=text-summary --report-dir=test-coverage/unit-tests npm run test:unit",
    "cover:unit:mocha": "mocha --require @babel/register 'server-side/tests/unit-tests/**/*.js' --timeout 10000 --exit --reporter mochawesome --reporter-options reportDir=test-reports/unit --reporter-options reportFilename=index",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize db:seed:all",
    "db:seed:undo": "npx sequelize db:seed:undo:all",
    "db:create": "npx sequelize db:create",
    "db:drop": "npx sequelize db:drop",
    "api:doc": "cd docs && aglio -i ./blueprint/input.apib --theme-template default --theme-variables cyborg -o index.html",
    "build": "npm run clean && babel server-side -d dist",
    "now-build": "npm run build",
    "clean": "rimraf dist",
    "prod": "npm run clean && npm run build && cd dist && node app.js",
    "test:unit:with:db": "NODE_ENV=test npm run test:setup && npm run test:unit && npm run test:drop:db ",
    "test:setup": "NODE_ENV=test npm run test:drop:db && npm run test:create:db && npm run test:migrate:db && npm run test:seed:db",
    "test:drop:db": "NODE_ENV=test npx sequelize-cli db:drop",
    "test:create:db": "NODE_ENV=test npx sequelize-cli db:create",
    "test:migrate:db": "NODE_ENV=test npx sequelize-cli db:migrate",
    "test:seed:db": "NODE_ENV=test npx sequelize-cli db:seed:all "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/make-kots-great-again/web.git"
  },
  "keywords": [
    "backend",
    "server"
  ],
  "author": "Daniel Olivier",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/client": "^7.2.1",
    "@sendgrid/mail": "^7.2.1",
    "accesscontrol": "^2.2.1",
    "babel-plugin-inline-dotenv": "^1.2.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "consola": "^2.11.3",
    "content-security-policy": "^0.3.3",
    "cors": "^2.8.5",
    "custom-env": "^2.0.1",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "expect-puppeteer": "^4.4.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.2",
    "mochawesome": "^6.1.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "puppeteer": "^3.0.4",
    "puppeteer-autoscroll-down": "^0.1.5",
    "readline": "^1.3.0",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.10.4",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.0",
    "@types/mongoose": "^5.7.3",
    "@types/bcrypt": "^3.0.0",
    "@types/sequelize": "^4.28.9",
    "istanbul": "^0.4.5",
    "nyc": "^15.0.1"
  }
}
