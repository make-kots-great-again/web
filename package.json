{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "node": "12.14.1"
  },
  "description": "back end server with node.js",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "npm run install",
    "start": "babel-node app.js",
    "db": "babel-node ./db/index.js",
    "dev": "nodemon -r dotenv/config --exec \"npm start\"",
    "build": "npm run clean && babel server-side -d dist",
    "now-build": "npm run build",
    "clean": "rimraf dist",
    "prod": "npm run clean && npm run build && cd dist && node app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danny00747/mean-stack-web-app.git"
  },
  "keywords": [
    "backend",
    "server"
  ],
  "author": "Daniel Olivier",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/client": "^7.2.1",
    "@sendgrid/mail": "^7.2.1",
    "@types/sequelize": "^4.28.9",
    "accesscontrol": "^2.2.1",
    "babel-plugin-inline-dotenv": "^1.2.2",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "consola": "^2.11.3",
    "content-security-policy": "^0.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "expect-puppeteer": "^4.4.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.2",
    "mochawesome": "^6.1.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "puppeteer": "^3.0.4",
    "puppeteer-autoscroll-down": "^0.1.5",
    "readline": "^1.3.0",
    "sequelize": "^6.3.5",
    "watch": "^1.0.2"
  },
